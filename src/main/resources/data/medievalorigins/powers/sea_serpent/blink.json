{
    "type": "origins:multiple",
    "active": {
        "type": "origins:active_self",
        "cooldown": 40,
        "key": {
            "key": "key.origins.secondary_active",
            "continuous": true
        },
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:or",
                "conditions": [
                    {
                        "type": "apoli:submerged_in",
                        "fluid": "minecraft:water"
                    },
                    {
                        "type": "apoli:in_rain"
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "*:*_w",
                            "comparison": ">",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "space": "local_horizontal_normalized",
                            "client": false,
                            "z": 7
                        }
                    },
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "*:*_a",
                            "comparison": ">",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "space": "local_horizontal_normalized",
                            "client": false,
                            "x": 7
                        }
                    },
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "*:*_s",
                            "comparison": ">",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "space": "local_horizontal_normalized",
                            "client": false,
                            "z": -7
                        }
                    },
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "*:*_d",
                            "comparison": ">",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "space": "local_horizontal_normalized",
                            "client": false,
                            "x": -7
                        }
                    }

                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:add_velocity",
                        "y": 1
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "power grant @p origins:fall_immunity"
                    },
                    {
                        "type": "origins:delay",
                        "ticks": 80,
                        "action": {
                            "type": "origins:execute_command",
                            "command": "power remove @p origins:fall_immunity"
                        }
                    }
                ]
            }
        }
    },
